{
  "name": "react-examples",
  "version": "1.0.0",
  "description": "Examples for [React](http://reactjs.com) and [ReactJS.NET](http://reactjs.net/).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/react-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/react-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/react-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx,.mjs --cache",
    "eslint-fix": "eslint . --ext .js,.jsx,.mjs --cache --fix",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,html,md,mjs,mts,ts,tsx}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,html,md,mjs,mts,ts,tsx}'",
    "start": "node \"./Util - 01 - Custom servers/server.js\" 8040",
    "validate": "npm run eslint && npm run prettier"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.25.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "prettier": "2.2.1",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@babel/core": "7.14.0",
    "@babel/preset-env": "7.14.1",
    "@babel/preset-react": "7.13.13",
    "@babel/standalone": "7.14.1",
    "core-js": "3.11.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recoil": "0.2.0",
    "serve": "11.3.2"
  }
}
